{
  "name": "fgit",
  "version": "2.0.0",
  "description": "A Git wrapper for conventional commits and other niceties",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "compile": "bun build ./src/index.ts --compile --outfile ./fgit",
    "dev": "bun run ./src/index.ts",
    "test": "bun test"
  },
  "bin": {
    "fgit": "./fgit"
  },
  "keywords": [
    "git",
    "conventional-commits",
    "cli"
  ],
  "author": "Daniel Westbrook",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}

